    "babel-preset-stage-3": "^6.5.0",
    "del": "^2.2.0",
    "rollup": "^0.33.0",
    "rollup-plugin-babel": "^2.4.0",
    "rollup-plugin-node-resolve": "^1.7.1"
  },
  "engines": {
    "node": ">=6.2.0"
  }
}

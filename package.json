{
  "name": "roleando",
  "version": "1.0.0",
  "description": "Herramientas RPG",
  "main": "bin/server.js",
  "scripts": {
    "build": "npm run prod:generators",
    "prod:generators": "browserify src/public/generadores/index.js -v -t rollupify -t babelify -t [ envify --NODE_ENV production ] --s Generators -v -o static/dist/generators.browser.js",
    "dev:generators": "watchify src/public/generadores/index.js -d -v -t rollupify -t babelify -t [ envify --NODE_ENV development ] --s Generators -o static/dist/generators.browser.js",
    "devstart": "nodemon -L bin/server",
    "start": "node bin/server",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "guumaster",
  "license": "ISC",
  "dependencies": {
    "@guumaster/rpg-generator-engine": "^0.5.1",
    "body-parser": "^1.15.1",
    "connect-mongo": "^1.2.0",
    "cookie-parser": "^1.4.3",
    "cors": "^2.7.1",
    "deep-assign": "^2.0.0",
    "ejs": "^2.4.2",
    "es6-promise": "^3.2.1",
    "express": "^4.13.4",
    "express-session": "^1.13.0",
    "isomorphic-fetch": "^2.2.1",
    "jsonwebtoken": "^7.0.0",
    "marked": "^0.3.5",
    "method-override": "^2.3.6",
    "mongodb": "^2.1.21",
    "mongoose": "^4.4.19",
    "morgan": "^1.7.0",
    "nodemailer": "^2.4.2",
    "nodemailer-sendgrid-transport": "^0.2.0",
    "nunjucks": "^2.4.2",
    "nunjucks-markdown": "^2.0.0",
    "passport": "^0.3.2",
    "passport-google-oauth": "^1.0.0",
    "require-dir": "^0.3.0",
    "restify-errors": "^4.2.3",
    "selectn": "^1.0.20",
    "serve-favicon": "^2.3.0",
    "shortid": "^2.2.6",
    "sitemap": "^1.6.0",
    "slug": "^0.9.1",
    "tv4": "^1.2.7"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-preset-es2015": "^6.9.0",
    "babelify": "^7.3.0",
    "browserify": "^13.0.1",
    "nodemon": "^1.9.2",
    "rollupify": "^0.3.1",
    "uglifyify": "^3.0.1",
    "watchify": "^3.7.0",
    "babel-plugin-transform-runtime": "^6.9.0",
